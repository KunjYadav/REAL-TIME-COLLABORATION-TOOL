{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "npm install",
        "buildCommand": "npm run build"
      }
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "build/static/$1"
    },
    {
      "src": "/[^.]+",
      "dest": "build/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "build/$1"
    }
  ]
}